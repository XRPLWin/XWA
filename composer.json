{
    "name": "xrplwin/xrplwinanalyzer",
    "version": "0.0.1",
    "type": "project",
    "description": "XRP Ledger Analyzer",
    "keywords": ["xrpl", "xrpl", "xrp-ledger", "analyzer", "php", "dynamodb", "aws"],
    "license": "MIT",
    "readmeFilename": "README.md",
    "bugs" : {
      "url" : "https://github.com/XRPLWin/xrplwinanalyzer/issues"
    },
    "require": {
        "php": "^8.0.2",
        "baopham/dynamodb": "^6.1",
        "brick/math": "^0.10.2",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^9.19"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "files": [
            "app/Helpers/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "XRPLWin\\XRPL\\": "packages/xrpl/src/",
            "XRPLWin\\XRPLOrderbookReader\\": "packages/xrpl-orderbook-reader/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
